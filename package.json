{
  "name": "memory-game",
  "version": "1.0.0",
  "description": "Memory card game with React and Webpack",
  "main": "src/index.js",
  "homepage": "https://olgagulyakevich.github.io/memory-game",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "deploy-full": "git add . && npm run build && git add dist -f && git commit -m 'Build for deployment' && git push origin main && npm run deploy"
  },
  "keywords": ["react", "game", "memory", "cards", "webpack"],
  "author": "Olga Gulyakevich <olga.gulyakevich@gmail.com>",
  "license": "MIT",
  "dependencies": {
  "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}