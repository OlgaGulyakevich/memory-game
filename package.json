{
  "name": "memory-game",
  "version": "1.0.0",
  "description": "Memory card game with React and Webpack",
  "main": "src/index.js",
  "homepage": "https://olgagulyakevich.github.io/memory-game",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:deploy": "GITHUB_PAGES=true webpack --mode production",
    "deploy": "npm run build:deploy && gh-pages -d dist",
    "serve": "npx serve -s dist -l 3000",
    "lighthouse": "npx lighthouse http://localhost:3000 --output html --view",
    "analyze": "webpack --mode production --analyze",
    "clean": "rm -rf dist node_modules package-lock.json && npm install"
  },
  "keywords": [
    "react",
    "game",
    "memory",
    "cards",
    "webpack"
  ],
  "author": "Olga Gulyakevich <olga.gulyakevi4@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "i18next": "^25.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "gh-pages": "^6.3.0",
    "html-webpack-plugin": "^5.6.4",
    "style-loader": "^4.0.0",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": false
}
